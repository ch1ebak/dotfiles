set nocompatible
filetype off
filetype plugin indent on
syntax on
set shiftwidth=4
set number
set ruler
set visualbell
set encoding=utf-8
set modelines=0
set wrap
set autoindent
set smartindent
set tabstop=2 shiftwidth=2 expandtab
set wildmenu
set wildmode=list:longest
set hidden
set ttyfast
set laststatus=2
set showmode
set showcmd
set number relativenumber
set nu rnu
